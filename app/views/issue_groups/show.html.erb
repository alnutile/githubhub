<div class="page-header">
  <%= link_to issue_groups_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_issue_group_path(@issue_group), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show issue_group</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @issue_group.title %></dd>

  <dt>Description:</dt>
  <dd><%= @issue_group.description %></dd>

</dl>
